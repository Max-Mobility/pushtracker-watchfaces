<?xml version="1.0" encoding="utf-8"?>
<drawable xmlns:app="http://schemas.android.com/apk/res-auto"
    class="android.support.wearable.complications.rendering.ComplicationDrawable"
    app:backgroundColor="@color/black"
    app:borderColor="@color/white"
    app:borderDashGap="3dp"
    app:borderDashWidth="2dp"
    app:borderRadius="50dp"
    app:borderStyle="dashed"
    app:borderWidth="1dp"
    app:highlightColor="@color/white"
    app:iconColor="@color/white"
    app:rangedValuePrimaryColor="@color/permobil_orange"
    app:rangedValueRingWidth="1dp"
    app:rangedValueSecondaryColor="@color/gray"
    app:textColor="@color/permobil_blue"
    app:textSize="15sp"
    app:textTypeface="@font/computer_robot"
    app:titleColor="@color/gray"
    app:titleSize="10sp"
    app:titleTypeface="@font/computer_robot">

    <ambient
        app:borderColor="@android:color/transparent"
        app:highlightColor="@color/white"
        app:iconColor="@color/white"
        app:rangedValuePrimaryColor="@color/white"
        app:rangedValueSecondaryColor="@android:color/transparent"
        app:titleColor="@color/white" />
</drawable>